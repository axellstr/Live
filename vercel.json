{
  "version": 2,
  "routes": [
    {
      "src": "/api/store-items",
      "dest": "/api/store-items.js"
    },
    {
      "src": "/api/create-checkout-session",
      "dest": "/api/create-checkout-session.js"
    },
    {
      "src": "/(.*)\\.html$",
      "dest": "/$1"
    }
  ],
  "rewrites": [
    { "source": "/:path*.html", "destination": "/:path*" }
  ],
  "redirects": [
    { "source": "/:path*.html", "destination": "/:path*", "permanent": true }
  ],
  "build": {
    "env": {
      "VERCEL_DISABLE_TOOLBAR": "1"
    }
  }
}
