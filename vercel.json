{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/eshop.html",
      "dest": "public/eshop.html"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}
