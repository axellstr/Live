{
  "version": 2,
  "routes": [
    {
      "src": "/api/store-items",
      "dest": "/api/store-items.js"
    },
    {
      "src": "/api/create-checkout-session",
      "dest": "/api/create-checkout-session.js"
    },
    {
      "src": "/eshop.html",
      "dest": "/eshop"
    },
    {
      "src": "/about.html",
      "dest": "/about"
    },
    {
      "src": "/contact.html",
      "dest": "/contact"
    },
    {
      "src": "/projects.html",
      "dest": "/projects"
    },
    {
      "src": "/index.html",
      "dest": "/"
    }
  ],
  "redirects": [
    {
      "source": "/eshop.html",
      "destination": "/eshop",
      "permanent": true
    },
    {
      "source": "/about.html",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/contact.html",
      "destination": "/contact",
      "permanent": true
    },
    {
      "source": "/projects.html",
      "destination": "/projects",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "build": {
    "env": {
      "VERCEL_DISABLE_TOOLBAR": "1"
    }
  }
}
