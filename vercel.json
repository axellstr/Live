{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/create-checkout-session",
      "dest": "server.js"
    },
    {
      "src": "/api/store-items",
      "dest": "/api/store-items.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
